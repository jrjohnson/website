    "node": ">= 18",
    "yarn": "use pnpm",
    "npm": "use pnpm"
  },
  "packageManager": "pnpm@8.6.12",
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "lib/markdown",
      "lib/splash"
    ]
  }
}
